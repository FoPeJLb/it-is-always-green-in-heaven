Print
 maim | xclip -selection clipboard -t image/png && \\
 paplay /usr/share/sounds/freedesktop/stereo/screen-capture.oga

alt + Print
 maim -i $(xdotool getactivewindow) | \\
 xclip -selection clipboard -t image/png && \\
 paplay /usr/share/sounds/freedesktop/stereo/screen-capture.oga

